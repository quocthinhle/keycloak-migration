{
  "name": "@teravn/keycloak-migration",
  "version": "20.0.0",
  "description": "Manage Keycloak migration",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tera-Group/keycloak-migration.git"
  },
  "author": "Khoi Pham <pckhoi@gmail.com> (https://github.com/pckhoi)",
  "type": "module",
  "keywords": [
    "keycloak",
    "migration"
  ],
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/index.js"
    }
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib && rimraf coverage",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean && echo Using TypeScript && tsc --version",
    "build:keycloak-image": "docker build -t kc_image:1 -f ../Dockerfile ..",
    "build": "tsc --pretty && tsc -p tsconfig.esm.json --pretty",
    "du": "docker-compose -f ../docker-compose.yml up --wait --build --force-recreate",
    "dd": "docker-compose -f ../docker-compose.yml down --rmi local",
    "test": "npm run build:keycloak-image && node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "watch": "npm run build -- --watch",
    "watch:test": "jest --watch"
  },
  "dependencies": {
    "axios": "^1.3.2",
    "testcontainers": "^10.4.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.2",
    "@keycloak/keycloak-admin-client": "^20.0.3",
    "@types/jest": "^23.3.3",
    "@types/node": "^10.11.4",
    "coveralls": "^3.0.2",
    "jest": "^29.4.2",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^7.0.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "bugs": {
    "url": "https://github.com/Tera-Group/keycloak-migration/issues"
  },
  "homepage": "https://github.com/Tera-Group/keycloak-migration#readme"
}
